<script>
    $(function() {
    // Récupération du formulaire
    var form = $('#my-form');
  
    // Ecouteur pour soumettre le formulaire
    $(form).submit(function(event) {
      event.preventDefault();
  
      // Récupération des données du formulaire
      var formData = $(form).serialize();
  
      // Envoi de la requête AJAX
      $.ajax({
        type: 'POST',
        url: '/web/user/login',
        data: formData
      })
      .done(function(response) {
        // Si la requête réussit, affiche le message de confirmation
        window.location.href="/dahsbordadmin/";
      })
      .fail(function(data) {
        // Si la requête échoue, affiche le message d'erreur et ajoute la classe "error" au formulaire
        var msg = JSON.parse(data.responseText);
        $('#error-message').text(msg.message);
        $(form).addClass('error');
        document.getElementById("pb").style.display = "block" 
      });
    });
  });
  
  function removeit(){
    document.getElementById("pb").style.display = "none"
    window.location.reload(); 
  }

  function togglePasswordVisibility() {
			  var passwordInput = document.getElementById("password");
			  if (passwordInput.type === "password") {
				passwordInput.type = "text";
			  } else {
				passwordInput.type = "password";
			  }
			}
  </script>
        
